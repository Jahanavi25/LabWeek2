package org.calculator;

public class Diesel extends Automobile {
	
	private int nWheels;
	private int cylinders;
	public Diesel(double range, int vin, double basePrice, String color, String mName,int nWheels, int cylinders) {
		super(range,vin,basePrice,color,mName);
		this.nWheels = nWheels;
		this.cylinders = cylinders;
	}
	public Diesel(){
		
	}
	public int getnWheels() {
		return nWheels;
	}
	public void setnWheels(int nWheels) {
		this.nWheels = nWheels;
	}
	public int getCylinders() {
		return cylinders;
	}
	public void setCylinders(int cylinders) {
		this.cylinders = cylinders;
	}
	


}

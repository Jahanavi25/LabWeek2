package org.calculator;

import java.util.Date;

public class Test {

	public static void main(String[] args) throws VehicleOutOfRangeException {
		// TODO Auto-generated method stub
		
		Standard s= new Standard(0,12,3000," black","ford",0.0,4,new Date());
		Electric e = new Electric(1,76,15000,"white","chrysler",153,false,2);
		Diesel d = new Diesel(2,54,8000,"red","fiat",50, 4, 3);
		SemiTruck st = new SemiTruck(3,66,20000,"silver","BMW",3000,4,5);
		
		Boat b = new Boat(BoatType.yacht,12,8047,985000,"white",60000);
		Boat b1 = new Boat(BoatType.speed,13,123,35000,"whiteblack",2200);
		Boat b2 = new Boat(BoatType.barge,14,893,500000,"silver",5000);
		Boat b3 = new Boat(BoatType.cargo,12,542,750000,"cream",85000);

		System.out.println(s.toString());
		System.out.println(e.toString());
		System.out.println(d.toString());
		System.out.println(st.toString());
		
		System.out.println(b.toString());
		System.out.println(b1.toString());
		System.out.println(b2.toString());
		System.out.println(b3.toString());
	}

}

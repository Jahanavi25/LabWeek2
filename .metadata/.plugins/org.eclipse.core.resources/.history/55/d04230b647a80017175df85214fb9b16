package PackCodeExercises;

public class SavingsAccountTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		SavingsAccount saver1 = new SavingsAccount(2000.0);
		SavingsAccount saver2 = new SavingsAccount(3000.0);
		SavingsAccount.modifyInterestRate(4.0);
		
		System.out.printf("Balance for Saver1 = %f\nBalance for Saver2 = %f\nInterest Rate = %f\n\n",saver1.getsavingsbalance()+saver1.monthlyinterest()+saver2.getsavingsbalance()+saver2.monthlyinterest());

	}

}

package PackCodeExercises;

public class Rectangle {
	private int length;
	private int width;
	
	public Rectangle(int lenght, int width){
		this.length= length;
		this.width = width;
		
	}

	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}

	public int getWidth() {
		return width;
	}

	public void setWidth(int width) {
		this.width = width;
	}
	
	public static int area(int l,int w){
		
		int a = l*w;
		return a;
	}

}
